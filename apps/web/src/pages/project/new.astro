---
import ProjectWizard from "../../islands/ProjectWizard.svelte";
const variant = Astro.url.searchParams.get("variant");
const wizardVariant = variant === "b" ? "B" : "A";
---
<html lang="en">
  <head>
    <title>New Project</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
  </head>
  <body>
    <ProjectWizard client:load wizardVariant={wizardVariant} />
  </body>
</html>
